#!/bin/bash

export ADDORUPDATEINICONFIG="$(basename "$0")"
export ADDORUPDATELINE_NO_APPEND=t

printUsage()
{
    containedOrAddOrUpdateIniConfig --help 2>&1 | sed \
	-e '/1	No modifications were made/{ N; s/.*/    1	No such SECTION \/ KEY, no modifications were made./; }'
}
case "$1" in
    --help|-h|-\?)	shift; printUsage "$0"; exit 0;;
esac

exec containedOrAddOrUpdateIniConfig "$@"
